<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>SearchFilter</filter-name>
        <filter-class>model.SearchFilter</filter-class>
    </filter>
    <filter>
        <filter-name>cat_fillter</filter-name>
        <filter-class>model.cat_fillter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>SearchFilter</filter-name>
        <servlet-name>movieId</servlet-name>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>
    <servlet>
        <servlet-name>movieId</servlet-name>
        <servlet-class>servlet.movieId</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newMovies</servlet-name>
        <servlet-class>servlet.newMovies</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>movieCategory</servlet-name>
        <servlet-class>servlet.movieCategory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>servlet.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteMovie</servlet-name>
        <servlet-class>servlet.deleteMovie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>page</servlet-name>
        <servlet-class>servlet.page</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testPath</servlet-name>
        <servlet-class>servlet.testPath</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>zxcazsc</servlet-name>
        <servlet-class>servlet.zxcazsc</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>movieId</servlet-name>
        <url-pattern>/movieId</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newMovies</servlet-name>
        <url-pattern>/newMovies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>movieCategory</servlet-name>
        <url-pattern>/movieCategory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteMovie</servlet-name>
        <url-pattern>/deleteMovie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>page</servlet-name>
        <url-pattern>/page</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testPath</servlet-name>
        <url-pattern>/testPath</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>zxcazsc</servlet-name>
        <url-pattern>/zxcazsc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
